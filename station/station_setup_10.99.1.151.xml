<?xml version="1.0" encoding="UTF-8"?>
<station>
  <label>Station One</label>
  <adapter>Parallel FPGA</adapter>
  <devices>

    <!-- Thermal Stream -->
    <device>
      <address>GPIB0::9</address>
      <manufacturer>Temptronic</manufacturer>
      <model>TP04300A</model>
      <part_code>4000</part_code>
    </device>

    <!-- Power Supplies -->
    <!-- AAAA BBBB -->
    <device>
      <address>GPIB0::1</address>
      <identifier>AB Power Supply</identifier>
      <manufacturer>Agilent Technologies</manufacturer>
      <model>E3648A</model>
      <part_code>0</part_code>
      <env_variable>
        <name>vddcore</name>
        <device_pointer>Output 1</device_pointer>
        <property>voltage</property>
      </env_variable>
      <env_variable>
        <name>vdda</name>
        <device_pointer>Output 2</device_pointer>
        <property>voltage</property>
      </env_variable>
    </device>
    <!-- CCCC DDDD -->
    <device>
      <address>GPIB0::2</address>
      <identifier>CD Power Supply</identifier>
      <manufacturer>Agilent Technologies</manufacturer>
      <model>E3648A</model>
      <part_code>0</part_code>
      <env_variable>
        <name>vddd</name>
        <device_pointer>Output 1</device_pointer>
        <property>voltage</property>
      </env_variable>
      <env_variable>
        <name>vddah</name>
        <device_pointer>Output 2</device_pointer>
        <property>voltage</property>
      </env_variable>
    </device>
    <!-- vdd25_tc -->
    <device>
      <address>GPIB0::23</address>
      <identifier>TC Power Supply</identifier>
      <manufacturer>Agilent Technologies</manufacturer>
      <model>E3631A</model>
      <part_code>0</part_code>
      <env_variable>
        <name>vdd25_tc</name>
        <device_pointer>Output P25V</device_pointer>
        <property>voltage</property>
      </env_variable>
    </device>





    <!-- Pattern Generator -->
    <device>
      <address>GPIB0::12</address>
      <manufacturer>Agilent Technologies</manufacturer>
      <model>81134A</model>
      <part_code></part_code>
    </device>
    
    <!-- Scopes -->
    <device>
      <address>GPIB0::8</address>
      <manufacturer>Tektronix</manufacturer>
      <model>CSA8000</model>
      <part_code></part_code>
    </device>
    
    <device>
      <address>GPIB0::3</address>
      <manufacturer>BERTScope</manufacturer>
      <model>12500A</model>
      <part_code></part_code>
    </device>
    
  </devices>
</station>